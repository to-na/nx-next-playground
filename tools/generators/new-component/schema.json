{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "new-article",
  "type": "object",
  "properties": {
    "componentName": {
      "type": "string",
      "description": "Component Name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "prj": {
      "type": "string",
      "description": "Project Name"
    },
    "dir": {
      "type": "string",
      "description": "Directory Name"
    },
    "required": ["componentName", "prj", "dir"]
  }
}
